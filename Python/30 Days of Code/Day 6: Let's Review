# https://www.hackerrank.com/challenges/30-review-loop/problem

n = int(input())
end_lst = []
for _ in range(n):
    word = input()
    add_lst = f"{''.join([word[i] for i in range(0, len(word), 2)])} {''.join([word[i] for i in range(1, len(word), 2)])}"
    end_lst.append(add_lst)
for result in end_lst:
    print(result)

