# https://www.hackerrank.com/challenges/python-mutations/problem
# Difficulty: Easy

string = input()
p = list(map(str, input().split()))

k = list(string)
for i in k:
    k[int(p[0])] = p[1]
print(("").join(k))

if __name__ == '__main__':
    s = input()
    i, c = input().split()
    s_new = mutate_string(s, int(i), c)
    print(s_new)
    
    
