# https://www.hackerrank.com/challenges/python-string-split-and-join/problem
# Difficulty: Easy

a = input()
def split_and_join(a):

    a = a.split(' ')
    a = '-'.join(a)
    return a
print(split_and_join(a))
if __name__ == '__main__':
    line = input()
    result = split_and_join(line)
    print(result)
